var win = Ti.UI.createWindow({
	layout : "vertical",
	backgroundColor : "#ccc"
	
});

var sym = Ti.UI.createLabel({
	left: 0,
	text: "Stock Symbol: "
	
});
var name = Ti.UI.createLabel({
	left: 0,
	text: "Stock Name: "
	
});
var price = Ti.UI.createLabel({
	left: 0,
	text: "Current Price: "
	
});
var hi = Ti.UI.createLabel({
	left: 0,
	text: "Price (High): "
	
});
var lo = Ti.UI.createLabel({
	left: 0,
	text: "Price (Low): "
	
});
var time = Ti.UI.createLabel({
	left: 0,
	text: "As Of: "
	
});
win.add(sym);
win.add(name);
win.add(price);
win.add(hi);
win.add(lo);
win.add(time);
win.open();

