var win = Ti.UI.createWindow({
	backgroundColor: "#f9f",
	layout: "vertical"
});
var name = Ti.UI.createLabel({
	color: "#fff",
	text: "Company Name: ",
	top: 40
});

var sym = Ti.UI.createLabel({
	color: "#fff",
	top: 10
	
});
var hi = Ti.UI.createLabel({
	color: "#fff",
	top: 10
	
	
});
var low = Ti.UI.createLabel({
	color: "#fff",
	top: 10
	
});
var buildUi = function(obj){
	console.log(obj);
	name.text = "Company Name: " + obj.name;
	sym.text = "Company Symbol: " + obj.sym;
	hi.text = "Company Hi: " + obj.hi;
	low.text = "Company Low: " + obj.low;
	win.add(hi);
	win.add(sym);
	win.add(name);
	win.open();
	
};
exports.buildUi = buildUi;