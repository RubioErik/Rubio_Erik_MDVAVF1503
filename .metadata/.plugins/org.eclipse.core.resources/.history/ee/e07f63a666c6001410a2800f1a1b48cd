var save = function(apiData) {
	console.log(apiData);
	var db = Ti.Database.open("stockDB");
	db.execute("CREATE TABLE IF NOT EXISTS stockTBL (id, INTEGER PRIMARY KEY, sym TEXT, name TEXT, price TEXT, hi TEXT, lo TEXT, time TEXT)");
	db.execute("INSERT INTO stockTBL (sym, name, price, hi, low, time) VALUES (?,?,?,?,?,?)", apiData.symbol, apiData.name, apiData.price, apiData.high, apiData.low, apiData.low);
};
exports.save;
