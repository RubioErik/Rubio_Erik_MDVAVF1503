var Cloud = require("ti.cloud");
function createUser(e) {
    Cloud.Users.login({
        username : "erubio12",
        password : "12345",
    }, function(e) {
        if (e.success) {
        	console.log("success");
            var api = require("api");
            api.getGeo();
        } else {
            alert("Error: " + e.message);
        }
    });
}