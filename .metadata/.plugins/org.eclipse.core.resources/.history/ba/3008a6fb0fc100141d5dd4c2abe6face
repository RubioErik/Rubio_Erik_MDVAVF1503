var getData = function() {
	console.log("getData");
	//request data
	// build object from data

	var url = "http://dev.markitondemand.com/Api/V2/Quote/json?symbol=NFLX";
	var xhr = Ti.Network.createHTTPClient({
		onload : function(evt) {
			console.log(evt);
			var json = JSON.parse(this.responseText);
			
			 var stockObj = {
			 	sym : json.Symbol,
			 	name : json.Name,
			 	hi : json.High,
			 	low : json.Low	
			 	
			 };
			
			 var uiFile = require("UI");
	         uiFile.buildUi(stockObj);
		},
		onerror : function(evt) {
			alert("Error: " + evt.error);
		}
	});
	xhr.open("GET", url);
	xhr.send();
	// pass object to UI module
	
};
exports.getData = getData; 