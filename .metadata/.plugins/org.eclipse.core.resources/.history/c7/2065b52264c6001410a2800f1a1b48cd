var getData = function(){
	var url = "http://dev.markitondemand.com/Api/v2/Quote/json?symbol=NFLX";
	var xhr = Ti.Network.createHTTPClient({
		onload : function(){
			var json = JSON.parse(this.responseText);
		},
		onerror : function() {
			alert("Data not available");
		},
		timeout: 5000
	});
	xhr.open("GET", url);
	xhr.send();
};
